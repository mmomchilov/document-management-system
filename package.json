{
  "name": "document-management-system",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rimraf": "rimraf",
    "wintersmith": "wintersmith",
    "changelog": "standard-changelog",
    "start:hmr": "ng serve --hmr",
    "start:aot": "ng serve --aot",
    "start:prod": "ng serve --prod",
    "start:prod:aot": "ng serve --prod --aot",
    "build": "npm run clean:dist && ng build --base-href='/ui/home/'",
    "build:prod": "npm run build -- --prod",
    "build:prod:aot": "npm run build:prod -- --aot",
    "build:ci": "npm run build:prod && npm run build:prod:aot",
    "build:demo": "npm run build:prod:aot",
    "upgrade": "ng update @angular/cli",
    "test:coverage": "npm run clean:coverage && ng test -sr -cc",
    "lint:styles": "stylelint ./src/app/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "clean:coverage": "npm run rimraf -- coverage",
    "docs:build": "wintersmith build -C docs -o build/ng2-admin",
    "docs:deploy": "npm run docs:build && gh-pages -d docs/build -r git@github.com:akveo/akveo.github.io.git -b master",
    "docs:serve": "wintersmith preview -C docs",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "rxjs": "^6.3.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1",
    "@ngx-translate/core": "11.0.1",
    "jsonpath": "1.0.2",
    "ngx-bootstrap": "5.5.0",
    "bootstrap": "4.0.0",
    "popper.js": "1.16.1",
    "jquery": "3.4.1",
    "mydatepicker": "2.6.9",
    "ng2-completer": "3.0.3",
    "ng2-smart-table": "1.5.0",
    "jquery-slimscroll": "^1.3.8",
    "ionicons": "5.0.1",
    "font-awesome": "4.7.0",
    "normalize.css": "8.0.1",
    "animate.css": "3.7.2",
    "flag-icon-css": "3.4.6",
    "roboto-fontface": "0.10.0"
  },
  "devDependencies": {
    "rimraf": "3.0.2",
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.29",
    "@types/jquery": "3.3.33",
    "@types/jquery.slimscroll": "1.3.31",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "^3.1.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
